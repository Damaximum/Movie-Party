<main>
<div class="row">
    <div class="col s12 m6">
        <ul class="tabs">
            <li class="tab col s6"><a href="#accepted"> Friends </a></li>
            <li class="tab col s6"><a href="#pending"> Pending </a></li>
        </ul>
    </div>
    <div id="accepted">
{{#each friend as |friend|}}

<li class="collection-item avatar">
    <span class="title">{{friend.user.name}}</span>
    <p>Status: {{friend.status}}</p>
    <div class="col s4">
        <a class="waves-effect waves-light btn" id="deleteFriend"><i class="material-icons right">clear</i>Delete</a>
    </div>
</li>

{{/each}}
</div>
</div>

<form>
        <div class="input-field">
          <input id="search" type="search" required>
          <label class="label-icon" for="search"><i class="material-icons">search</i></label>
          <i class="material-icons">close</i>
        </div>
      </form>

</main>